package p;
import java.util.ArrayList;
import java.util.List;
public class test 
{
	public static List<Integer> turn(String s1)
	{
		List<Integer> L=new ArrayList<Integer>();
		int x1=0,j=0;
		while(j<s1.length())
		{
			if(s1.charAt(j)!='.')
			{
				x1=x1*10+s1.charAt(j)-'0';
				//j++;
			}
			else
			{
				//System.out.println("x1="+x1);
				L.add(x1);
				x1=0;
			}
			j++;
		}
		
		return L;
	}
	public static int compareVersion(String version1, String version2) 
	{
		version1=version1+".";
		version2=version2+".";
		List<Integer> L1=new ArrayList<Integer>();
		List<Integer> L2=new ArrayList<Integer>();
		List<Integer> Ls;
		L1=turn(version1);
		L2=turn(version2);
		int flag=0;
		int length=L1.size()<L2.size()?L1.size():L2.size();
		int length1=L1.size()>=L2.size()?L1.size():L2.size();
		if(L1.size()>L2.size())
		{
			Ls=new ArrayList<Integer>(L1);
			flag=1;
		}
		else
		{
			Ls=new ArrayList<Integer>(L2);
			flag=-1;
		}
		int i=0;
		for(i=0;i<length;i++)
		{
			if(L1.get(i)<L2.get(i))
				return -1;
			else if((L1.get(i)>L2.get(i)))
				return 1;
		}
		for(int k=i;k<length1;k++)
		{
			if(Ls.get(k)>0)
			{
				return flag;
			}
		}
		return 0;
    }
	
	public static void main(String[] args) 
	{
		System.out.println(compareVersion("1.100","1.0002"));
		
	}
	
}
